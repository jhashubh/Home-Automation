<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/aa">

    <ListView
        android:id="@+id/listview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:textColor="@android:color/white" />
        android:background="@drawable/a">

        <TextView
            android:id="@+id/t1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/switch1"
            android:layout_alignBottom="@+id/switch1"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="20dp"
            android:layout_marginStart="20dp"
            android:padding="2dp"
            android:textColor="@color/colorAccent"
            android:textSize="25sp" />

        <Switch
            android:id="@+id/switch1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_marginEnd="24dp"
            android:layout_marginRight="24dp"
            android:layout_marginTop="54dp"
            android:textOff="OFF"
            android:textOn="ON"
            android:thumb="@drawable/customswitchselector"
            android:track="@drawable/control_track" />

        <Switch
            android:id="@+id/switch2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/switch1"
            android:layout_alignStart="@+id/switch1"
            android:layout_below="@+id/switch1"
            android:layout_marginTop="28dp"
            android:textOff="OFF"
            android:textOn="ON"
            android:thumb="@drawable/customswitchselector"
            android:track="@drawable/control_track" />

        <Switch
            android:id="@+id/switch3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/switch2"
            android:layout_alignStart="@+id/switch2"
            android:layout_below="@+id/switch2"
            android:layout_marginTop="32dp"
            android:textOff="OFF"
            android:textOn="ON"
            android:thumb="@drawable/customswitchselector"
            android:track="@drawable/control_track" />

        <Switch
            android:id="@+id/switch4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/switch3"
            android:layout_alignStart="@+id/switch3"
            android:layout_below="@+id/switch3"
            android:layout_marginTop="32dp"
            android:textOff="OFF"
            android:textOn="ON"
            android:thumb="@drawable/customswitchselector"
            android:track="@drawable/control_track" />

        <Switch
            android:id="@+id/switch5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/switch4"
            android:layout_alignStart="@+id/switch4"
            android:layout_below="@+id/switch4"
            android:layout_marginTop="41dp"
            android:textOff="OFF"
            android:textOn="ON"
            android:thumb="@drawable/customswitchselector"
            android:track="@drawable/control_track" />

        <TextView
            android:id="@+id/t2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/t1"
            android:layout_alignStart="@+id/t1"
            android:layout_alignTop="@+id/switch3"
            android:padding="2dp"
            android:textColor="@color/colorAccent"
            android:textSize="26sp" />

        <TextView
            android:id="@+id/t3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/t2"
            android:layout_alignStart="@+id/t2"
            android:layout_alignTop="@+id/switch4"
            android:padding="2dp"
            android:textColor="@color/colorAccent"
            android:textSize="26sp" />

        <TextView
            android:id="@+id/t4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/t3"
            android:layout_alignStart="@+id/t3"
            android:layout_alignTop="@+id/switch5"
            android:textColor="@color/colorAccent"
            android:textSize="26sp" />


    </RelativeLayout>
</LinearLayout>